package com.gxz.bus.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;

@Controller
@RequestMapping("bus")
public class BusController {

	/**
	 * 去客户管理
	 */
	@RequestMapping("toCustomerManager")
	public String toCustomerManager() {
		return "bus/customerManager";
	}
	
	
	/**
	 * 去供应商管理
	 */
	@RequestMapping("toProviderManager")
	public String toProviderManager() {
		return "bus/providerManager";
	}
	
	/**
	 * 去商品管理
	 */
	@RequestMapping("toGoodsManager")
	public String toGoodsManager() {
		return "bus/goodsManager";
	}
	
	/**
	 * 去进货管理
	 */
	@RequestMapping("toInportManager")
	public String toInportManager() {
		return "bus/inportManager";
	}
	
	/**
	 * 去退货管理
	 */
	@RequestMapping("toOutportManager")
	public String toOutportManager() {
		return "bus/outportManager";
	}
	
	/**
	 * 去销售管理
	 */
	@RequestMapping("toSalesManager")
	public String toSalesManager() {
		return "bus/sales/salesManager";
	}
	
	@RequestMapping("toSalesLeftManager")
	public String toSalesLeftManager() {
		return "bus/sales/salesManagerLeft";
	}
	
	@RequestMapping("toSalesRightManager")
	public String toSalesRightManager() {
		return "bus/sales/salesManagerRight";
	}
}
